{{ ansible_managed | comment }}

#loglevel.default is the default log level for jobs: ERROR,WARN,INFO,VERBOSE,DEBUG
loglevel.default= {{ rundeck_loglevel }}

rdeck.base= {{ rundeck_rdeckbase }}

#rss.enabled if set to true enables RSS feeds that are public (non-authenticated)
rss.enabled= {{ rundeck_rss_enable }}

# change hostname here
grails.serverURL=  {{ rundeck_url }}

dataSource.dbCreate= {{ rundeck_datasource_dbcreate }}
dataSource.url= {{ rundeck_datasource_url }}
#dataSource.url= jdbc:h2:file:/var/lib/rundeck/data/rundeckdb;MVCC=true
dataSource.username={{ rundeck_datasource_username }}
dataSource.password={{ rundeck_datasource_password }}
dataSource.driverClassName={{ rundeck_datasource_driverclassname }}

rundeck.log4j.config.file= /etc/rundeck/log4j.properties
